@Article{Lissitz1976,
author="Lissitz, Robert W.
and Sch{\"o}nemann, Peter H.
and Lingoes, James C.",
title="A solution to the weighted procrustes problem in which the transformation is in agreement with the loss function",
journal="Psychometrika",
year="1976",
volume="41",
number="4",
pages="547--550",
abstract="This paper provides a generalization of the Procrustes problem in which the errors are weighted from the right, or the left, or both. The solution is achieved by having the orthogonality constraint on the transformation be in agreement with the norm of the least squares criterion. This general principle is discussed and illustrated by the mathematics of the weighted orthogonal Procrustes problem.",
issn="1860-0980",
doi="10.1007/BF02296976",
url="http://dx.doi.org/10.1007/BF02296976"
}
@Article{Koschat1991,
author="Koschat, Martin A.
and Swayne, Deborah F.",
title="A weighted procrustes criterion",
journal="Psychometrika",
year="1991",
volume="56",
number="2",
pages="229--239",
abstract="The Procrustes criterion is a common measure for the distance between two matricesX andY, and can be interpreted as the sum of squares of the Euclidean distances between their respective column vectors. Often a weighted Procrustes criterion, using, for example, a weighted sum of the squared distances between the column vectors, is called for. This paper describes and analyzes the performance of an algorithm for rotating a matrixX such that the column-weighted Procrustes distance toY is minimized. The problem of rotatingX intoY such that an aggregate measure of Tucker's coefficient of congruence is maximized is also discussed.",
issn="1860-0980",
doi="10.1007/BF02294460",
url="http://dx.doi.org/10.1007/BF02294460"
}

